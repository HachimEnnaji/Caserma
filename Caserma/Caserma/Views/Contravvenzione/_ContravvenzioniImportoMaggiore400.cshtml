@model IEnumerable<Caserma.Models.Contravvenzione>

<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.T_Cognome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.T_Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.V_DataViolazione)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.V_DecurtamentoPunti)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.V_Importo)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.T_Cognome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.T_Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.V_DataViolazione)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.V_DecurtamentoPunti)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.V_Importo)
            </td>

        </tr>
    }

</table>